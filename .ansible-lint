---
profile: production
exclude_paths:
  - roles/*/files/
strict: true
quiet: false
verbosity: 1
offline: false
skip_list:
  - yaml[indentation]
  - var-naming[no-role-prefix]
  - no-changed-when
  - name[template]
  - command-instead-of-shell
  - command-instead-of-module
  - risky-file-permissions
